{
  "schemaVersion": 1,
  "id": "autologin",
  "version": "${version}",
  "name": "AutoLogin",
  "description": "A Fabric Mod that automatically logs in when entering a server with authentication",
  "authors": ["lushangkan"],
  "contact": {
    "repo": "https://github.com/lushangkan/AutoLogin"
  },
  "license": "GPL-3.0",
  "icon": "assets/autologin/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "cn.cutemc.autologin.AutoLogin"
    ],
    "modmenu": [
      "cn.cutemc.autologin.config.ConfigMenuIntegration"
    ]
  },
  "mixins": [
    "autologin.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}",
    "cloth-config": "*",
    "modmenu": "*"
  }
}
